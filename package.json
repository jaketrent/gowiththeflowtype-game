{
  "name": "gowiththeflowtype-game",
  "version": "1.0.0",
  "private": true,
  "description": "Codebase to demonstrate Flowtype",
  "main": "dist/index.js",
  "scripts": {
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "precommit": "lint-staged",
    "start": "run-p start:client start:server",
    "start:server": "json-server server/db.js --port 1338",
    "start:client": "parcel serve -p 1337 static/index.html",
    "test": "run-s test:types test:code",
    "test:code": "jest",
    "test:types": "flow",
    "test:watch": "npm run test -- --watch"
  },
  "keywords": [],
  "author": "jaketrent",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-angular": "^6.1.3",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "flow-bin": "^0.69.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.4",
    "npm-run-all": "^4.1.2",
    "parcel-bundler": "1.5.1",
    "prettier": "^1.11.1"
  },
  "lint-staged": {
    "*.js": [
      "node_modules/.bin/prettier --single-quote --no-semi --write",
      "git add"
    ]
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "flow-typed": "^2.4.0",
    "json-server": "^0.12.1",
    "lit-html": "^0.9.0",
    "node-fetch": "^2.1.2",
    "page": "^1.8.6"
  }
}
